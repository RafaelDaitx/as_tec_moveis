<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rafaelLucasASRamon.cripto">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Cripto">
        <activity
            android:name="com.rafaelLucasASRamon.criptomoeda.TransactionsDetails"
            android:parentActivityName="com.rafaelLucasASRamon.criptomoeda.MainActivity"
            android:exported="true" />
        <activity
            android:name="com.rafaelASRamon.criptomoeda.BuyCrypto"
            android:exported="true"
            android:parentActivityName="com.rafaelASRamon.criptomoeda.SelectCrypto" />
        <activity
            android:name="com.rafaelASRamon.criptomoeda.SelectCrypto"
            android:exported="true"
            android:parentActivityName="com.rafaelLucasASRamon.criptomoeda.MainActivity" />
        <activity android:name="com.rafaelLucasASRamon.criptomoeda.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>